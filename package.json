{
	"name": "mandrasch.dev",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide"
	},
	"devDependencies": {
		"@iconify-json/skill-icons": "^1.1.8",
		"@inlang/paraglide-js": "1.7.2",
		"@inlang/paraglide-js-adapter-sveltekit": "^0.6.7",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.2.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/enhanced-img": "^0.3.9",
		"@sveltejs/kit": "^2.5.27",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/eslint": "^8.56.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.45.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.2.6",
		"sass": "^1.76.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.1",
		"typescript": "^5.5.0",
		"unplugin-icons": "^0.19.0",
		"vite": "^5.4.4"
	},
	"type": "module",
	"dependencies": {
		"@picocss/pico": "^2.0.6",
		"mdsvex": "^0.11.0",
		"svelte-icons": "^2.1.0"
	}
}
